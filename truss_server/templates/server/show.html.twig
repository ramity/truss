{% extends 'base.html.twig' %}

{% block title %}Server{% endblock %}

{% block body %}
    <h1>Server</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ server.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ server.name }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ server.createdAt ? server.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ server.updatedAt ? server.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_server_index') }}">back to list</a>

    <a href="{{ path('app_server_edit', {'id': server.id}) }}">edit</a>

    {{ include('server/_delete_form.html.twig') }}
{% endblock %}
